def count_until_larger(num): #0given
!BLANK = 0  #1given #blankcnt
!BLANK = num % 10  #1given #blankk
while !BLANK:  #1given #blanknum > 0
if num % 10 > !BLANK:  #2given #blankk
return !BLANK  #3given #blankcnt
!BLANK //= !BLANK  #2given #blanknum #blank10
!BLANK += !BLANK  #2given #blankcnt #blank1
return !BLANK  #1given #blank-1
print('DEBUG:', !BLANK)
print('DEBUG:', !BLANK)
